{
    "name": "aicost",
    "version": "1.2.2",
    "description": "An utility for calculating the cost of AI models",
    "repository": "https://github.com/meistrari/aicost",
    "scripts": {
        "release": "bunx bumpp && npm publish",
        "build": "bunx unbuild",
        "index": "bun run scripts/index-model-info.ts",
        "postinstall": "mise-en-place postinstall"
    },
    "keywords": [],
    "author": "Henrique Cunha <henrique@meistrari.com>",
    "license": "ISC",
    "devDependencies": {
        "@meistrari/mise-en-place": "2.9.0",
        "@types/bun": "latest",
        "bumpp": "^9.3.0",
        "typescript": "^5.3.3",
        "unbuild": "^2.0.0"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/index.mjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    }
}
